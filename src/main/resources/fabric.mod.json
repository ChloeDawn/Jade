{
  "schemaVersion": 1,
  "id": "jade",
  "version": "${version}",

  "name": "Jade",
  "description": "Minecraft mod shows what you are looking at. (Hwyla fork)",
  "authors": ["Snownee"],
  "contributors": ["TehNut", "ProfMobius", "kalkafox"],
  "icon": "icon.png",
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/jade",
    "sources": "https://github.com/Snownee/Jade",
    "issues": "https://github.com/Snownee/Jade/issues"
  },

  "license": "CC BY-NC-SA 4.0",

  "entrypoints": {
    "main": [
      "snownee.jade.Jade"
    ],
    "client": [
      "snownee.jade.JadeClient"
    ],
    "jade": [
      "snownee.jade.addon.core.CorePlugin",
      "snownee.jade.addon.vanilla.VanillaPlugin",
      "snownee.jade.addon.universal.UniversalPlugin"
    ],
    "modmenu": [
      "snownee.jade.compat.ModMenuCompat"
    ],
    "jei_mod_plugin": [
      "snownee.jade.compat.JEICompat"
    ]
  },
  "mixins": [ "jade.mixins.json" ],
  "accessWidener" : "jade.accesswidener",

  "depends": {
    "fabric-api": "*",
    "minecraft": "1.18.2",
    "java": ">=17"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/KzGQW7a"
      }
    }
  }
}
